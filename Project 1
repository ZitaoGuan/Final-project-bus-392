def read_file():
    pass


def highest_closing(list_val):
    high_value = 0
    date = 0
    for val in list_val:
        if val[4] > high_value:
            high_value = val[4]
            date = val[0]
    return (date, high_value)


def lowest_closing(list_val):
    low_value = 0
    date = 0
    for val in list_val:
        if val[1] < low_value:
            low_value = val[1]
            date = val[0]
    return (date, low_value)


open_files()
